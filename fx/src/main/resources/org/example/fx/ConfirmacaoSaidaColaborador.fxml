        <Pane layoutX="290.0" layoutY="171.0" prefHeight="169.0" prefWidth="379.0" style="-fx-background-color: white;">
            <children>
                <Text layoutX="65.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tem a certeza que pretende sair?">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Text>
                <Button fx:id="sair" onAction="#logoutButton" layoutX="238.0" layoutY="102.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="97.0" style="-fx-background-color: rgb(255,0,0);" text="SIM" textFill="WHITE" />
                <Button fx:id="cancelar" onAction="#cancelarButton" layoutX="67.0" layoutY="102.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="97.0" style="-fx-background-color: rgb(102,51,0) rgb(102,51,0);" text="CANCELAR" textFill="WHITE" />
            </children>
        </Pane>
    </children>

</AnchorPane>
